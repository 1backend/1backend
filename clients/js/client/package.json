{
  "name": "@openorch/client",
  "version": "0.3.0-rc.29",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && rollup -c"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "typescript",
    "library",
    "module",
    "commonjs",
    "esm"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/openorch/openorch.git"
  },
  "author": "Janos Dobronszki",
  "license": "AGPL-3.0-only",
  "description": "JS/TS client for OpenOrch. Works both from the browser and from Node.js",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@types/axios": "^0.14.0",
    "rollup": "^3.0.0",
    "rollup-plugin-typescript2": "^0.31.1",
    "typescript": "^5.5.3"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "dependencies": {
    "axios": "^1.7.2",
    "glob": "^11.0.0",
    "tslib": "^2.8.1"
  }
